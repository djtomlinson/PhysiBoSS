Node BMP4{
  logic = BMP4;
  rate_up  = @logic ? $time_scale : 0;
  rate_down  = @logic ? $return_prob : $time_scale;
}

Node WNT{
  logic = (WNT & BMP4);
  rate_up  = @logic ? $time_scale : 0;
  rate_down  = @logic ? $return_prob : $time_scale;
}

Node NODAL{
  logic = (WNT & ($auto_inhib)) OR (NODAL & ($auto_inhib));
  rate_up  = @logic ? $time_scale : 0;
  rate_down  = @logic ? $return_prob : $time_scale;
}
